#include <iostream>
using namespace std;


  int fibonacci(int x)
  {
    int f;
    if (x==1){
      f=1;
}
    else if (x==0){
      f=0;
    }

      else  {
      int a=1;
      int b=0;
      f = 1;
      int g;

      while (a<b){
        g = f;
        f= f+b;
        b= g;
        a++;
      }
    }

  return f;
  }



  int main()
  {
   cout<<"Type number to do the Fibonacci function"<<endl;
   int k;
   cin>>k;
   int d = fibonacci(k);
   cout<<d<<endl;
   return 0;
  }
